{
  "expo": {
    "name": "T-D App",
    "slug": "t-d_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Simple To-Do application for creating colour coded lists with tasks using Firebase.",
    "githubUrl": "https://github.com/GoranACM/TODOApp-RN-Firebase"
  }
}
